    <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="#//Proposition"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="And" eSuperTypes="#//Proposition">
