package eval.expr_cst_mult_add.aspects

import fr.inria.diverse.k3.al.annotationprocessor.Aspect
import expr_cst_mult_add.Expr
import expr_cst_mult_add.Add
import expr_cst_mult_add.Mult
import expr_cst_mult_add.Cst

import static extension eval.expr_cst_mult_add.aspects.ExprAspect.*
import static extension eval.expr_cst_mult_add.aspects.AddAspect.*
import static extension eval.expr_cst_mult_add.aspects.MultAspect.*
import static extension eval.expr_cst_mult_add.aspects.CstAspect.*

@Aspect(className=Expr)
abstract class ExprAspect {

}

@Aspect(className=Add)
class AddAspect extends ExprAspect {

}

@Aspect(className=Mult)
class MultAspect extends ExprAspect {

}

@Aspect(className=Cst)
class CstAspect extends ExprAspect {

}



